# ---------------------------------------------------------------------------------------------------------------------
# DEFINE REGION
# ---------------------------------------------------------------------------------------------------------------------
provider "aws" {
  region = "eu-west-2"
}
# ---------------------------------------------------------------------------------------------------------------------
# CREATE S3 Bucket
# ---------------------------------------------------------------------------------------------------------------------
 resource "aws_s3_bucket" "b" {
  bucket = "amirco99-bucket"
  acl    = "public-read-write"

  tags {
    Name        = "My bucket"
   Environment = "Dev"
  }
}
